{
  "manifest_version": 2,

  "name": "Save Tabs to Bookmarks",
  "description": "Saves all your open tabs into bookmarks.",
  "version": "1.0",

  "permissions": [
    "bookmarks",
    "tabs"
  ],
  
  "browser_action": {
    "default_icon": "iconB.png"
  },
  
  "icons": { "16": "iconB.png",
           "48": "iconB48.png"},
           
  "background": {
    "scripts": ["createBookmarks.js"]
  }
}